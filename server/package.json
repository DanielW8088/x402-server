{
  "name": "x402-token-mint-server",
  "version": "1.0.0",
  "description": "Token minting server - pay 1 USDC to mint tokens (x402 enabled)",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "dev": "tsx watch index-multi-token.ts",
    "dev:lp-deployer": "tsx watch lp-deployer-standalone.ts",
    "dev:ai-mint": "tsx watch ai-mint-executor.ts",
    "start:lp-deployer": "tsx lp-deployer-standalone.ts",
    "start": "tsx index-multi-token.ts",
    "start:ai-mint": "node dist/ai-mint-executor.js",
    "build": "tsc",
    "check": "tsx checkPendingTx.ts",
    "check:role": "tsx checkRole.ts"

 },
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.22.0",
    "@coinbase/x402": "latest",
    "@types/cors": "^2.8.19",
    "@types/ioredis": "^4.28.10",
    "@types/pg": "^8.11.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.8.2",
    "openai": "^6.7.0",
    "pg": "^8.13.1",
    "viem": "^2.21.54",
    "x402-express": "latest"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
